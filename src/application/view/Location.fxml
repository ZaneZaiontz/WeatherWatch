<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="800.0" maxWidth="800.0" minHeight="800.0" minWidth="800.0" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.LocationController">
   <children>
      <BorderPane layoutX="250.0" layoutY="75.0" prefHeight="125.0" prefWidth="300.0" AnchorPane.topAnchor="75.0">
         <top>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter City Name" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Arial" size="24.0" />
               </font>
            </Text>
         </top>
         <bottom>
            <Button fx:id="submit" mnemonicParsing="false" onAction="#handle" style="-fx-background-color: #909090; -fx-background-radius: 0;" text="Search" textFill="WHITE" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <TextField fx:id="city" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="200.0" promptText="Enter City" style="-fx-background-radius: 0; -fx-background-color: #909090;" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Arial Italic" size="12.0" />
               </font>
            </TextField>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
